{% extends "KsUserBundle::layout.html.twig" %}

{% block title %}
	{{ parent() }}
{% endblock %}
    
{% block content %}

<h1>InvitationEmailBeta list</h1>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>Could_invit</th>
            <th>Nomber_invitation</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('invitationemailbeta_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.email }}</td>
            <td>{{ entity.couldinvit }}</td>
            <td>{{ entity.nomberinvitation }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('invitationemailbeta_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('invitationemailbeta_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('invitationemailbeta_new') }}">
            Create a new entry
        </a>
    </li>
</ul>

{% endblock %}