{{ leagueLevel.category.label|trans }} 
{% if leagueLevel.starNumber > 0 %} 

    {% for i in range(2, 3 - leagueLevel.starNumber) %} 
        <i class="icon-star"></i>
    {% endfor %}
{% endif %}
{% if leagueLevel.starNumber < 3 %} 
    {% for i in range(1, 3 - leagueLevel.starNumber) %} 
        <i class="icon-star-empty"></i>
    {% endfor %}
{% endif %}